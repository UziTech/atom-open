{
  "name": "open",
  "main": "index.js",
  "version": "2.0.4",
  "description": "Open a file in Atom with a URL",
  "keywords": [
    "open"
  ],
  "repository": "https://github.com/UziTech/atom-open",
  "license": "MIT",
  "engines": {
    "atom": ">=1.25.0"
  },
  "uriHandler": {
    "method": "handleURI"
  },
  "configSchema": {
    "confirmBeforeOpen": {
      "type": "boolean",
      "default": true,
      "title": "Confirm",
      "description": "Confirm before opening the file?"
    },
    "openProject": {
      "type": "boolean",
      "default": false,
      "title": "Open Project",
      "description": "Open the project folder as well as the file if the file is part of a project?"
    }
  },
  "atomTestRunner": "atom-jasmine3-test-runner",
  "scripts": {
    "test": "atom --test spec",
    "lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^6.0.2",
    "atom-jasmine3-test-runner": "^4.3.10",
    "eslint": "^6.8.0",
    "semantic-release": "^16.0.0"
  },
  "dependencies": {
    "promisificator": "^4.1.1"
  }
}
