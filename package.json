{
  "name": "open",
  "main": "index.js",
  "version": "0.1.1",
  "description": "Open a file in Atom with a URL",
  "keywords": [
    "open"
  ],
  "repository": "https://github.com/UziTech/atom-open",
  "license": "MIT",
  "engines": {
    "atom": ">=1.25.0"
  },
  "uriHandler": {
    "method": "handleURI"
  },
  "configSchema": {
    "confirmBeforeOpen": {
      "type": "boolean",
      "default": true,
      "title": "Confirm",
      "description": "Confirm before opening the file?"
    },
    "openProject": {
      "type": "boolean",
      "default": false,
      "title": "Open Project",
      "description": "Open the project folder as well as the file if the file is part of a project?"
    }
  }
}
